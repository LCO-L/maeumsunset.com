{
  "name": "sunset",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    // 기본 빌드 (out/ 생성)
    "build": "next build",
    // out/을 docs/로 정적 export
    "export": "next export -o docs",
    // 한 번에 끝내기: build + export + CNAME + .nojekyll
    "build:docs": "npm run build && npm run export && echo maeumsunset.com > docs/CNAME && touch docs/.nojekyll"
  }
}
,
  "dependencies": {
    "idb": "^8.0.3",
    "next": "15.4.6",
    "next-pwa": "^5.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
